{
  "path-intellisense.mappings": {
    "@components": "${workspaceRoot}/src/components",
    "@navigators": "${workspaceRoot}/src/navigators",
    "@reducers": "${workspaceRoot}/src/reducers",
    "@res": "${workspaceRoot}/src/res",
    "@sagas": "${workspaceRoot}/src/sagas",
    "@services": "${workspaceRoot}/src/services",
    "@themes": "${workspaceRoot}/src/themes",
    "@views": "${workspaceRoot}/src/views",
  },
  "java.configuration.updateBuildConfiguration": "interactive",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  }
}
